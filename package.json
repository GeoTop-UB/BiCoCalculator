{
	"name": "bbcalculator",
	"private": true,
	"version": "0.2.0",
	"type": "module",
	"scripts": {
		"dev-server": "cross-env PUBLIC_ADAPTER=node vite dev",
		"dev-static": "cross-env PUBLIC_ADAPTER=static vite dev",
		"build-server": "cross-env PUBLIC_ADAPTER=node vite build",
		"build-static": "cross-env PUBLIC_ADAPTER=static vite build",
		"preview-server": "cross-env PUBLIC_ADAPTER=node vite preview",
		"preview-static": "cross-env PUBLIC_ADAPTER=static vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint ."
	},
	"dependencies": {
    	"cross-env": "^10.1.0"
	},
	"devDependencies": {
		"@eslint/compat": "^1.2.5",
		"@eslint/js": "^9.22.0",
		"@fontsource/fira-mono": "^5.0.0",
		"@neoconfetti/svelte": "^2.0.0",
		"@sveltejs/adapter-static": "^3.0.8",
		"@sveltejs/adapter-node": "^5.4.0",
		"@sveltejs/kit": "^2.22.0",
		"@sveltejs/vite-plugin-svelte": "^6.0.0",
		"@types/node": "^22",
		"eslint": "^9.22.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-svelte": "^3.0.0",
		"globals": "^16.0.0",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"svelte": "^5.25.0",
		"svelte-check": "^4.0.0",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.20.0",
		"vite": "^7.0.4",
		"mathlifier": "^3.2.2",
		"axios": "^1.12.2",
		"sockjs-client": "^1.6.1",
		"object-hash": "^3.0.0"
	}
}
